{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngz-identity-utils": {
      "projectType": "library",
      "root": "projects/ngz-identity-utils",
      "sourceRoot": "projects/ngz-identity-utils/src",
      "prefix": "ngz",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ngz-identity-utils/tsconfig.lib.json",
            "project": "projects/ngz-identity-utils/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngz-identity-utils/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngz-identity-utils/src/test.ts",
            "tsConfig": "projects/ngz-identity-utils/tsconfig.spec.json",
            "karmaConfig": "projects/ngz-identity-utils/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ngz-identity-utils/tsconfig.lib.json",
              "projects/ngz-identity-utils/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "ngz-identity-utils",
  "cli": {
    "analytics": "550737ca-87c7-41bc-9b22-e148783e2473"
  }
}